package searching;

import java.lang.System.Logger;

public class LinearSearch {
	public static void main(String args[]) {
		int[] arr = {1,2,3,4,5};
		Logger logger 
		if(linearSearch(arr, 3))
			logger.info("Present");
		else 
			System.out.println("Not present");
	}
	public static boolean linearSearch(int arr[],int n)
	{
		for(int e: arr) {
			if(e==n)
				return true;
		}
			return false;
	}
}
