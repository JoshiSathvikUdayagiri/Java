package sorting;

public class SelectionSort {
	public static void main(String[] args) {
		int[] arr = {5,1,9,5,7,10,11,12,15,24,3};
		int[] result = selectionSort(arr);
		for(int e: result) {
			System.out.println(e);
		}
	}
